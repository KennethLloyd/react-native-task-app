{
  "name": "express-sequelize-starter",
  "version": "2.1.0",
  "type": "module",
  "description": "NodeJS Express Boilerplate with Sequelize as ORM",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=production node -r dotenv/config src/index.js",
    "db-migrate": "sequelize db:create && sequelize db:migrate",
    "db-seed": "sequelize db:seed:all",
    "dev": "nodemon -r dotenv/config src/index.js",
    "docs": "apidoc -i src/controllers -o docs",
    "lint": "eslint --no-error-on-unmatched-pattern src/**",
    "lint-fix": "eslint --no-error-on-unmatched-pattern src/** --fix",
    "test": "mocha './src/tests/**/*.js'"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "apidoc": {
    "title": "API Documentation"
  },
  "keywords": [
    "api",
    "boilerplate",
    "express",
    "jwt",
    "node",
    "restful",
    "sequelize"
  ],
  "author": "Kenneth Lloyd Verastigue",
  "license": "ISC",
  "dependencies": {
    "apidoc": "^0.26.0",
    "apollo-server-express": "^2.22.1",
    "cors": "^2.8.5",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "apollo-server-testing": "^2.22.2",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "sinon": "^10.0.0"
  }
}
